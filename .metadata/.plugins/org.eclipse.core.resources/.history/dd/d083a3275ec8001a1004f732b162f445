@SpringBootApplication
public class TrainEnquiryServiceApplication {
	
	@Value("${train-enquiry-service.pages}")
	private int pages;

	public static void main(String[] args) {
		SpringApplication.run(TrainEnquiryServiceApplication.class, args);
	}

}
